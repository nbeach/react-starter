{
  "private": true,
  "scripts": {
    "start": "node dev-server.js",
    "build": "parcel build src/index.html",
    "test": "mocha --watch",
    "test:once": "mocha",
    "test:e2e": "mocha src/**/*.e2e-test.ts",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "compile": "tsc --noEmit",
    "storybook": "start-storybook -s ./public",
    "ci-checks": "yarn compile && yarn test:once && yarn lint",
    "ci-checks:e2e": "yarn start & wait-on 'http://localhost:1234' && yarn test:e2e && killall node"
  },
  "mocha": {
    "extension": [
      "ts",
      "tsx"
    ],
    "reporter": "progress",
    "spec": "./src/**/*.test.*",
    "watch-files": [
      "./src/**/*.ts",
      "./src/**/*.tsx"
    ],
    "require": [
      "./src/ts-node-register",
      "src/test-setup.ts"
    ]
  },
  "dependencies": {
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@storybook/react": "^6.0.28",
    "@types/chai": "^4.2.14",
    "@types/jsdom": "^16.2.5",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.4",
    "@types/ramda": "0.27.32",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-redux": "7.1.11",
    "@types/redux-mock-store": "1.0.2",
    "@types/redux-saga": "^0.10.5",
    "@types/selenium-webdriver": "^4.0.10",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.1",
    "chai": "^4.2.0",
    "chromedriver": "~86.0.0",
    "dom-sim": "^1.0.0",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.20",
    "mocha": "^8.2.1",
    "moment": "^2.29.1",
    "parcel-bundler": "^1.12.4",
    "parcel-proxy-server": "^1.1.0",
    "redux-mock-store": "1.5.4",
    "sass": "^1.29.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "ts-node": "9.0.0",
    "tslint": "^6.1.0",
    "tslint-immutable": "^6.0.1",
    "typescript": "4.0.5",
    "wait-on": "5.2.0"
  }
}
