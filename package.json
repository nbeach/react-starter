{
  "private": true,
  "scripts": {
    "start": "node dev-server.js",
    "build": "parcel build src/index.html",
    "test": "mocha --watch",
    "test:once": "mocha",
    "test:e2e": "mocha src/**/*.e2e-test.ts",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "compile": "tsc --noEmit",
    "storybook": "start-storybook -s ./public",
    "ci-checks": "yarn compile && yarn test:once && yarn lint",
    "ci-checks:e2e": "yarn start & wait-on 'http://localhost:1234' && yarn test:e2e && killall node"
  },
  "mocha": {
    "extension": [
      "ts",
      "tsx"
    ],
    "reporter": "progress",
    "spec": "./src/**/*.test.*",
    "watch-files": [
      "./src/**/*.ts",
      "./src/**/*.tsx"
    ],
    "require": [
      "./src/ts-node-register",
      "src/test-setup.ts"
    ]
  },
  "dependencies": {
    "moment": "^2.24.0",
    "parcel-proxy-server": "^1.1.0",
    "ramda": "^0.27.0",
    "react": "^16.8.3",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@storybook/react": "^5.3.17",
    "@types/chai": "^4.2.11",
    "@types/jsdom": "^16.1.0",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/ramda": "^0.26.44",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.3",
    "@types/redux-mock-store": "^1.0.0",
    "@types/redux-saga": "^0.10.5",
    "@types/selenium-webdriver": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chromedriver": "~86.0.0",
    "dom-sim": "^1.0.0",
    "jsdom": "^16.2.1",
    "lodash": "^4.17.15",
    "mocha": "^7.1.0",
    "parcel-bundler": "^1.12.4",
    "redux-mock-store": "^1.5.3",
    "sass": "^1.24.3",
    "selenium-webdriver": "^4.0.0-alpha.3",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "tslint-immutable": "^6.0.1",
    "typescript": "^3.8.3",
    "wait-on": "^5.2.0"
  }
}
