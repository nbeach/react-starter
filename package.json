{
  "private": true,
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "test": "mocha --watch",
    "test:once": "mocha",
    "test:e2e": "mocha src/**/*.e2e-test.ts",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --fix --ext .ts,.tsx,.js",
    "compile": "tsc --noEmit",
    "storybook": "export NODE_OPTIONS=--openssl-legacy-provider; start-storybook -p 6006",
    "ci-checks": "yarn compile && yarn test:once && yarn lint",
    "ci-checks:e2e": "yarn start & wait-on 'http://localhost:1234' && yarn test:e2e && killall node",
    "build-storybook": "build-storybook"
  },
  "mocha": {
    "extension": [
      "ts",
      "tsx"
    ],
    "reporter": "progress",
    "spec": "./src/**/*.test.*",
    "watch-files": [
      "./src/**/*.ts",
      "./src/**/*.tsx"
    ],
    "require": [
      "./src/ts-node-register",
      "jsdom-global/register"
    ]
  },
  "dependencies": {
    "ramda": "0.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "redux": "4.2.1",
    "redux-saga": "1.2.2",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/ramda": "0.28.23",
    "@types/react-dom": "18.0.11",
    "@types/react-redux": "7.1.25",
    "@types/redux-mock-store": "1.0.3",
    "@types/redux-saga": "0.10.5",
    "@types/selenium-webdriver": "4.1.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "^8.3.0",
    "chai": "4.3.7",
    "chromedriver": "^110.0.0",
    "eslint": "8.34.0",
    "eslint-plugin-functional": "5.0.4",
    "jsdom": "21.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "10.2.0",
    "parcel": "2.8.3",
    "process": "^0.11.10",
    "redux-mock-store": "1.5.4",
    "selenium-webdriver": "4.8.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "wait-on": "7.0.1"
  }
}
